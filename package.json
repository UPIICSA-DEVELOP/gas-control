  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "gulp clean && ng build --prod && ng run com.maplander.app.inspector.front:server:production && npm run config:prod && npm run api && npm run deeplinks:prod && npm run server",
    "build:develop": "gulp clean && ng build -c develop && ng run com.maplander.app.inspector.front:server:develop && npm run api && npm run config:dev && npm run deeplinks:dev && npm run server",
    "test-ssr": "gulp clean && ng build -c develop && ng run com.maplander.app.inspector.front:server:develop && npm run api && npm run config:dev && npm run deeplinks:dev && npm run server && node dist/server",
    "server": "webpack --mode production --config server/webpack.config.js --progress --colors",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "deploy:develop": "gulp zip --file dist && node hooks/scp --filePath dist/bundle.zip --destination /var/www/html/app.inspector.com/deploy && node hooks/remotely --env dev",
    "deploy:production": "gulp zip --file dist && node hooks/scp --filePath dist/bundle.zip --destination /var/www/html/app.inspector.com/deploy && node hooks/remotely --env prod",
    "config:dev": "set NODE_ENV=dev&& node server/config.js",
    "config:prod": "set NODE_ENV=prod&& node server/config.js",
    "prerelease:patch": "standard-version --release-as patch --prerelease beta",
    "prerelease:minor": "standard-version --release-as minor --prerelease beta",
    "prerelease:major": "standard-version --release-as major --prerelease beta",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "api": "gulp api && webpack --mode production --config api/webpack.config.js --progress --colors",
    "deeplinks:dev": "set NODE_ENV=dev&& node deep-links/deep-links.js",
    "deeplinks:prod": "set NODE_ENV=prod&& node deep-links/deep-links.js",
    "setup-deploy": "gulp zip --file hooks/bash && node hooks/scp --filePath hooks/bash/bundle.zip --destination /var/www/html/app.inspector.com/deploy/bash && node hooks/remotely --setup",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
