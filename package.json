  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "test:ssr": "gulp clean && ng build -c develop --stats-json && ng run com.maplander.inspector.front:server:develop && npm run postbuild:test:ssr",
    "postbuild:test:ssr": "npm run webpack:server",
    "build:develop": "gulp clean && ng build -c develop ---deploy-url https://storage.googleapis.com/inspector-maplander-develop.appspot.com/cdn/ && ng run com.maplander.inspector.front:server:develop && npm run postbuild:develop",
    "postbuild:develop": "npm run upload:cdn:dev && gulp clean:source && npm run create:nconfig:dev && npm run webpack:server",
    "prebuild": "npm run version | gulp clean",
    "build": "ng build --prod ---deploy-url https://storage.googleapis.com/maplander-public.appspot.com/cdn/ && ng run com.maplander.inspector.front:server:production",
    "postbuild": "npm run upload:cdn:prod && gulp clean:source && npm run create:nconfig:prod && npm run webpack:server",
    "webpack:server": "webpack --mode production --config webpack.config.js --progress --colors && node dist/server",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "deploy:develop": "gcloud app deploy ./dist/app.yaml --project=inspector-maplander-develop",
    "deploy:production": "gcloud app deploy ./dist/app.yaml --project=maplander-public",
    "upload:cdn:dev": "set NODE_ENV=dev&& node upload.js",
    "upload:cdn:prod": "set NODE_ENV=prod&& node upload.js",
    "create:nconfig:dev": "set NODE_ENV=dev&& node nconfig.js",
    "create:nconfig:prod": "set NODE_ENV=prod&& node nconfig.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "start cmd.exe /K node version"
  },
