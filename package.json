  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "test:ssr": "gulp clean && ng build -c develop && ng run com.maplander.app.inspector.front:server:develop && npm run copy:endpoints && npm run config:dev && npm run deep:links && npm run server && node dist/server",
    "build:develop": "gulp clean && ng build -c develop && ng run com.maplander.app.inspector.front:server:develop && npm run copy:endpoints && npm run config:dev && npm run deep:links && npm run server",
    "build": "gulp clean ng build --prod && ng run com.maplander.app.inspector.front:server:production && npm run config:prod && npm run copy:endpoints && npm run deep:links && npm run server",
    "server": "webpack --mode production --config server/webpack.config.js --progress --colors",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "deploy:develop": "gulp zip --file dist && node hooks/scp --filePath dist/bundle.zip --destination /var/www/html/app.inspector.com/deploy && node hooks/remotely --env dev",
    "deploy:production": "gulp zip --file dist && node hooks/scp --filePath dist/bundle.zip --destination /var/www/html/app.inspector.com/deploy && node hooks/remotely --env prod",
    "config:dev": "set NODE_ENV=dev&& node server/config.js",
    "config:prod": "set NODE_ENV=prod&& node server/config.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "start cmd.exe /K node version",
    "copy:endpoints": "gulp copy:endpoints",
    "deep:links": "node deep-links/deep-links.js",
    "setup:deploy": "gulp zip --file hooks/config && node deploy/scp --filePath hooks/config/bundle.zip --destination /var/www/html/app.inspector.com/deploy/config && node hooks/remotely --setup"
  },
